<!DOCTYPE html>
<!-- Website template by freewebsitetemplates.com -->
<html>
<head>
	<meta charset="UTF-8">
	<title>{{title}}</title>
	<link rel="stylesheet" href="css/style.css" type="text/css">
</head>
<body>
	<div id="background">
		<div id="page">
			{{> header}}
			<div id="contents">
				<div class="box">
					<div>
						<div id="contact" class="body">
							<h1>Contact</h1>
							{{> contactform}}
							<h2>Travlr Getaways</h2>
							{{#each contactinfo}}
							<p>
								<span>{{this.title}}</span> {{this.info}}
							</p>
							{{/each}}
						</div>
					</div>
				</div>
			</div>
		</div>
            {{> footer}}
	</div>
</body>
</html>